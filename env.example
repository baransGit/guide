# Sydney Guide - Environment Variables Template
# Bu dosyayi .env olarak kopyalay ve gercek degerlerle doldur

# Google Maps API  
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# NSW Transport API
NSW_TRANSPORT_API_KEY=your_nsw_transport_api_key_here

# Claude API (Anthropic)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Push Notifications
FIREBASE_SERVER_KEY=your_firebase_server_key_here

# Database Configuration
DATABASE_URL=sqlite:///./sydney_guide.db

# MCP Server Configuration
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8888
MCP_SERVER_DEBUG=true

# App Configuration
APP_NAME=Sydney Guide
APP_VERSION=1.0.0
APP_ENV=development

# Logging
LOG_LEVEL=INFO
LOG_FILE=sydney_guide.log 